{
	"compilerOptions": {
		"target": "es5",
		"lib": ["es5", "es6", "dom"],
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": false,
		"noEmit": true,
		"outDir": "./dist/",
		"jsx": "react-jsx",
		"noImplicitAny": true,
		"baseUrl": "./src/app/",
		"paths": {
			"@common/*": ["common/*"],
			"@components/*": ["components/*"],
			"@hooks/*": ["hooks/*"],
			"@pages/*": ["pages/*"],
			"@redux/*": ["redux/*"],
			"@routes/*": ["routes/*"],
			"@styled/*": ["styled/*"],
			"@services/*": ["services/*"]
		}
	},
	"include": ["./src/**/*"]
}
